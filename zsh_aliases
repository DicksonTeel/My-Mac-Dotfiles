alias top="btop"
alias htop="btop"
alias btm="btop"
alias cat="bat"
alias update="brew update && brew upgrade && brew cleanup && mas outdated && mas upgrade"
alias ls="ls -al"
alias home="cd ~"
alias nettest='ping -c 10 1.1.1.1 && echo " " && traceroute 1.1.1.1 && echo " " && speedtest'
alias cleanup="brew cleanup && sudo periodic daily weekly monthly"
alias clear="clear && neofetch"
alias df="df -h"
alias symlink="ln -sf"
alias lux="lux -m"
alias parent="cd .."
